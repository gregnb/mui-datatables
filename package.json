  "peerDependencies": {
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "react": "^16.8.0 || ^17.0.2",
    "react-dom": "^16.8.0 || ^17.0.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.16.3",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
