    "react-to-print": "^2.14.1"
  },
  "side-effects": false,
  "nyc": {
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
