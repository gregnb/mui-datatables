{
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  },
  "presets": [
    "@babel/react",
    ["@babel/env", {
      "targets": {
        "browsers": ["> 1%", "iOS >= 8", "Android >= 4"],
        "node": "6.10"
      },
      "debug": false
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
